<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

    <skipIfNotSupported/>

    <programGroup defaultName="Ecvideo" location="applications"/>

      <shortcut
        name="Ecvideo Application"
        programGroup="yes"
        desktop="yes"
        applications="no"
        startMenu="yes"
        startup="no"
        target="$INSTALL_PATH\gvod.jar"
        commandLine=""
        description="Ecvideo video-on-deman"
        iconFile="$INSTALL_PATH\exe.ico"
